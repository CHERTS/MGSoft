  MGPlacementFormStorageEditor in '..\MGPlacementFormStorageEditor.pas' {FormStorageDesigner},
  MGPlacementMinMaxEditor in '..\MGPlacementMinMaxEditor.pas' {MGMinMaxInfoEditDialog};

end.
